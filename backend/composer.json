{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "~8.2.0",
        "ext-ctype": "*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-random": "*",
        "api-platform/core": "^3.1.14",
        "doctrine/dbal": "^3.6.5",
        "doctrine/doctrine-bundle": "^2.10.1",
        "doctrine/doctrine-migrations-bundle": "^3.2.4",
        "doctrine/orm": "^2.16.0",
        "doctrine/persistence": "^3.2.0",
        "lexik/jwt-authentication-bundle": "^2.19.1",
        "symfony/asset": "^6.3.0",
        "symfony/cache": "^6.3.2",
        "symfony/config": "^6.3.2",
        "symfony/console": "^6.3.2",
        "symfony/contracts": "^3.3.0",
        "symfony/dependency-injection": "^6.3.2",
        "symfony/dotenv": "^6.3.0",
        "symfony/event-dispatcher": "^6.3.2",
        "symfony/expression-language": "^6.3.0",
        "symfony/flex": "^2.3.3",
        "symfony/framework-bundle": "^6.3.2",
        "symfony/http-client": "^6.3.2",
        "symfony/http-kernel": "^6.3.3",
        "symfony/mailer": "^6.3.0",
        "symfony/mime": "^6.3.3",
        "symfony/monolog-bundle": "^3.8",
        "symfony/password-hasher": "^6.3.0",
        "symfony/runtime": "^6.3.2",
        "symfony/security-bundle": "^6.3.3",
        "symfony/security-core": "^6.3.3",
        "symfony/serializer": "^6.3.3",
        "symfony/templating": "^6.3.0",
        "symfony/translation": "^6.3.3",
        "symfony/twig-bridge": "^6.3.2",
        "symfony/twig-bundle": "^6.3.0",
        "symfony/validator": "^6.3.2",
        "symfony/yaml": "^6.3.3",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "hautelook/alice-bundle": "^2.12",
        "justinrainbow/json-schema": "^5.2.12",
        "maglnet/composer-require-checker": "^4.6",
        "phpstan/phpstan": "^1.10.26",
        "phpstan/phpstan-doctrine": "^1.3.40",
        "phpunit/phpunit": "^10.2.6",
        "slevomat/coding-standard": "^8.13.2",
        "squizlabs/php_codesniffer": "^3.7.2",
        "symfony/browser-kit": "^6.3.2",
        "symfony/css-selector": "^6.3.2",
        "symfony/debug-bundle": "^6.3.2",
        "symfony/maker-bundle": "^1.50",
        "symfony/stopwatch": "^6.3.0",
        "symfony/var-dumper": "^6.3.3",
        "symfony/web-profiler-bundle": "^6.3.2",
        "vimeo/psalm": "^5.13.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "install-codestandards": [
            "PHPCSStandards\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.3.*",
            "docker": false
        }
    }
}
